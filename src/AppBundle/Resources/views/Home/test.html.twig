{% extends '::base.html.twig' %}

{% block body %}
    <ul>
        {% for group in questionGroups %}
             {% for question in group %}
                 <li>
                 <div>
                     {{ loop.index }}{{ question.content|raw }}
                 </div>
                 <div>
                     <hr>
                     {% for answer in question.answers %}
                         <label><input type="checkbox" title=""> {{ answer|raw }}</label><br>
                     {% endfor %}
                 </div>
                 </li>
             {% endfor %}
        {% endfor %}
    </ul>
{% endblock %}